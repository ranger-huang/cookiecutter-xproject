    "timezone": "Asia/Shanghai",
    "use_celery": "n",
    "use_docker": "n",
    "use_compressor": "n",
    "postgresql_version": ["9.6", "9.5", "9.4", "9.3", "9.2"],
    "mysql_version": ["5.5", "5.6", "5.7", "8"],
